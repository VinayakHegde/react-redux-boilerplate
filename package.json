{
  "name": "react-redux-boilerplate",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://vinayakhegde.github.io/react-redux-boilerplate",
  "dependencies": {
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-redux": "^7.1.3",
    "react-scripts": "3.2.0",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "npm run predeploy && gh-pages -d build",
    "lint": "eslint --fix src",
    "prettify": "prettier --write \"src/**/*.+(js|jsx|json|css|scss|md)\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint": "^6.6.0",
    "gh-pages": "^2.1.1",
    "node-sass": "^7.0.0",
    "prettier": "^1.19.1",
    "redux-devtools-extension": "^2.13.8",
    "redux-mock-store": "^1.5.3"
  }
}
